<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="vna_electricity_number_report_form" model="ir.ui.view">
            <field name="name">vna_electricity_number_report_form</field>
            <field name="model">vna.electricity.number.report</field>
            <field name="arch" type="xml">
                <form string="Expense Report">
                    <group col="4">
                        <field name="from_date"/>
                        <field name="to_date"/>
                        <field name="hotel_room_type_ids" widget="many2many_tags" options="{'no_create': True}"/>
                        <field name="company_id"/>
                    </group>
                    <footer>
                        <button name="action_print" string="Print Excel" class="oe_highlight" type="object"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="vna_electricity_number_report_act" model="ir.actions.act_window">
            <field name="name">Electricity Number Report</field>
            <field name="res_model">vna.electricity.number.report</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="vna_electricity_number_report_form"/>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_vna_electricity_number_report" name="Electricity Number Report"
                  sequence="0" parent="account.account_reports_management_menu"
                  action="vna_electricity_number_report_act"/>
    </data>
</odoo>